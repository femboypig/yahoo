{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "yahoo",
  "version": "0.1.0",
  "identifier": "com.yahoo.app",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "titleBarStyle": "Overlay",
        "hiddenTitle": true,
        "fullscreen": false,
        "height": 800,
        "resizable": true,
        "title": "Yahoo",
        "width": 1280,
        "minHeight": 700,
        "minWidth": 1100,
        "visible": true,
        "zoomHotkeysEnabled": false,
        "decorations": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/icon.ico"
    ]
  }
}