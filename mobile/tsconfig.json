{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-native",
    "baseUrl": ".",
    "paths": {
      "*": [
        "src/*"
      ]
    },
    "typeRoots": [
      "./src/types"
    ]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "src/types/*.d.ts",
    "nativewind-env.d.ts"
  ]
}